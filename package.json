{
  "name": "@nhost/nhost-js",
  "version": "0.2.0",
  "description": "Nhost JavaScript SDK",
  "keywords": [
    "nhost"
  ],
  "author": "Nhost",
  "homepage": "https://github.com/nhost/nhost-js",
  "bugs": "https://github.com/nhost/nhost-js/issues",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "run-s build:esm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test:backend:start": "cd test/backend; nhost dev -d",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhost/nhost-js.git"
  },
  "dependencies": {
    "@nhost/hasura-auth-js": "^0.1.0",
    "@nhost/hasura-storage-js": "^0.0.5",
    "axios": "^0.23.0",
    "jwt-decode": "^3.1.2",
    "query-string": "^7.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.2"
  }
}
